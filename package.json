{
	"name": "api-rest-typescript",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon --exec ts-node ./src/index.ts",
		"docker:run": "docker-compose up -d",
		"migration:dev": "prisma migrate dev",
		"migration:deploy": "prisma migrate deploy",
		"build": "yarn add typescript --dev && rm -rf ./dist && tsc && bundle:adminjs && yarn remove typescript",
		"bundle:adminjs": "yarn add ts-node @tiptap/pm --dev && ts-node ./src/admin/bundler.ts && mv .adminjs/components.bundle.js .adminjs/bundle.js && yarn remove ts-node @tiptap/pm",
		"start": "node ./dist/index.js"
	},
	"devDependencies": {
		"@tiptap/pm": "^2.0.0",
		"@types/express": "^4.17.13",
		"@types/express-jwt": "^7.4.2",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/mongodb": "^4.0.7",
		"@types/node": "^18.0.1",
		"nodemon": "^2.0.19",
		"prisma": "^4.11.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@adminjs/bundler": "^2.0.0",
		"@adminjs/express": "^5.1.0",
		"@adminjs/import-export": "^2.0.2",
		"@adminjs/passwords": "^3.0.1",
		"@adminjs/prisma": "^3.0.1",
		"@prisma/client": "^4.11.0",
		"adminjs": "^6.8.7",
		"argon2": "^0.30.3",
		"chart.js": "^4.2.1",
		"dotenv": "^16.0.1",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"express-formidable": "^1.2.0",
		"express-jwt": "^8.4.1",
		"express-session": "^1.17.3",
		"ioredis": "^5.3.1",
		"jsonwebtoken": "^9.0.0",
		"openai": "^3.2.1",
		"pg": "^8.7.3",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-redux": "^8.0.5",
		"tslib": "^2.5.0"
	}
}
